<?xml version="1.0" encoding="UTF-8"?>  

<widget xmlns ="http://www.w3.org/ns/widgets"
 xmlns:gap ="http://phonegap.com/ns/1.0"
 id = "com.phonegap.app1"
 version ="1.0.0">
 
 <name> app1-2019 Datos del Dispositivo </name>
 
 <description>
 Aplicacion que nos muestra la informacion refente al Dispositivo Movil
 </description>
 
 <author href= "https://build.phonegap.com" email="cb16.lezama.flores.esmeralda1f@gmail.com">
 Esmeralda Lezama Flores
 </author>
 
 <!--Personaliza la aplicacion y plataforma-->
 <preference name="orientation"  value="default"/>
 <!--preference name="orientation"  value="portrait"/>
 <preference name="orientation"  value="landscape"/>-->
 <preference name="target-device"  value="universal"/>
 <preference name="fullscreen"  value="true"/>
 <preference name="android-installLocation"  value="auto"/>
 <preference name="permissions"  value="INTERNET"/>
 
 <!--Plugins-->
 <plugin name="cordova-plufin-device" spec="~1.1.4">
 <plugin name="cordova-plufin-vibration" spec="~2.1.3">
 <plugin name="cordova-plufin-device" spec="~1.3.1">
 
 <!--<plugin name="cordova-plufin-geolocation" source="npm"/>-->
 
 
 <!--colocar icono app-->
 
 <icon src="res/android/mdpi.png"/>
 
 </widget>
 